{
  "name": "slack-gitlab",
  "private": false,
  "version": "0.2.0",
  "description": "Slack-Gitlab Notifier",
  "repository": {
    "url": "https://github.com/brunojdo/slack-gitlab-notifier.git",
    "type": "git"
  },
  "scripts": {
    "prestart": "yarn",
    "start": "nodemon express.js -e yml,js,json --exec babel-node",
    "build": "babel *.js -d dist",
    "serve": "node dist/express.js"
  },
  "devDependencies": {
    "babel-loader": "7.1.x",
    "babel-preset-es2015": "6.24.x",
    "babel-preset-stage-2": "6.24.x",
    "express": "4.10.x",
    "fs": "0.0.1-security",
    "gitlab-webhook-handler2": "0.1.2",
    "js-yaml": "3.9.x",
    "slackbots": "1.0.2"
  },
  "dependencies": {
    "lodash": "4.17.x",
    "node-gitlab-api": "1.1.x"
  }
}
